function isperfect(n){
  let sum = 0;
  for(let i =1; i<n;i++){
    if(n%i == 0){
      sum += i
    }
  }
  if(sum == n){
    return "perfect number"
  }
  else if(sum>n){
    return "Abundant"
  }
  
  return "Deficient"
}

console.log(isperfect(6));

console.log(isperfect(12));

console.log(isperfect(8));
