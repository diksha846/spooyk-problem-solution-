var res = [];
function noofcups(  cups , k  ,  g ,  b )
{
	while (cups)
	{
		  tmp = k ; 
		if ( g>0)
		{
			while ( tmp--)
			{
				if ( g<=0)
					return -1 ;
					res.push("Green");  
				g-- ;
				cups--;  
			}
		}
		tmp = k ; 
		if ( b>0)
		{
			while ( tmp--)
			{
				if ( b<=0)
					return -1 ; 
				res.push("black"); 
				b-- ; 
				cups--; 
			}
		}
	}
	return res;

} 
console.log(noofcups(5,1,3,2));
console.log(noofcups(4,3,4,0));
